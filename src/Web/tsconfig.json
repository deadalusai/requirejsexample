{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "experimentalDecorators": true,
        "importHelpers": true,
        "jsx": "react",
        "baseUrl": "./Scripts",
        "outDir": "./wwwroot/js",
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2015.promise",
            "es2015.iterable"
        ]
    },
    "include": [
        "./Scripts/**/*",
        "./custom_types/**/*"
    ]
}