{
  "name": "web",
  "version": "0.0.1",
  "description": "RequireJS + .NET Core Example",
  "scripts": {
    "postinstall": "jspm install",
    "build": "tsc -p . && node optimize.js",
    "watch": "tsc -p . --sourceMap --inlineSources --watch"
  },
  "author": "Benjamin Fox",
  "devDependencies": {
    "jspm": "^0.16.53",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@types/bootstrap": "^3.3.32",
    "@types/jquery": "^2.0.40",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "jspm": {
    "directories": {
      "baseURL": "wwwroot/js"
    },
    "configFile": "wwwroot/_jspm.config.js",
    "dependencies": {
      "@angular/common": "npm:@angular/common@^4.1.3",
      "@angular/compiler": "npm:@angular/compiler@^4.1.3",
      "@angular/core": "npm:@angular/core@^4.1.3",
      "@angular/platform-browser": "npm:@angular/platform-browser@^4.1.3",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^4.1.3",
      "angular-core": "npm:@angular/core@^4.1.3",
      "bootstrap": "github:twbs/bootstrap@3.3.6",
      "es6-promise": "npm:es6-promise@^4.1.0",
      "es6-shim": "github:es-shims/es6-shim@^0.35.3",
      "jquery": "npm:jquery@2.2.4",
      "react": "npm:react@15.3.1",
      "react-dom": "npm:react-dom@^15.5.4",
      "reflect-metadata": "npm:reflect-metadata@^0.1.10",
      "rxjs": "npm:rxjs@^5.4.0",
      "tslib": "npm:tslib@^1.7.1",
      "zone.js": "npm:zone.js@^0.8.11"
    },
    "devDependencies": {}
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
