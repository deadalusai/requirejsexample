{
  "name": "web",
  "version": "0.0.1",
  "description": "RequireJS + .NET Core Example",
  "scripts": {
    "postinstall": "bower install && bower-installer",
    "build": "tsc -p . && node optimize.js",
    "watch": "tsc -p . --sourceMap --inlineSources --watch"
  },
  "author": "Benjamin Fox",
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/jquery": "^2.0.40",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "almond": "^0.3.3",
    "bower": "^1.7.9",
    "bower-installer": "^1.2.0",
    "jspm": "^0.16.53",
    "requirejs": "^2.3.1",
    "typescript": "^2.3.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "wwwroot/js"
    },
    "configFile": "wwwroot/_jspm.config.js",
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@3.3.6",
      "es6-promise": "npm:es6-promise@^4.1.0",
      "jquery": "npm:jquery@2.2.4",
      "react": "npm:react@15.3.1",
      "react-dom": "npm:react-dom@^15.5.4",
      "tslib": "npm:tslib@^1.7.1"
    },
    "devDependencies": {}
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
